{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts"]
}
